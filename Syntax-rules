
       // comment
        
       // ; end instruction
       a = b+c;
       
       // continuation character
       Stringa = 'Prima riga -
            seconda riga (segno -)';
         // -> result 'Prima riga     seconda riga (segno -)'      
       Stringa = 'Prima riga +
            seconda riga (segno +)';
         // -> result 'Prima riga seconda riga (segno +)'
